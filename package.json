{
  "type": "module",
  "name": "hoosat-create",
  "version": "1.0.0",
  "description": "Hoosat Create is React SSR & CSR Stack made by Hoosat Oy",
  "author": "",
  "license": "MIT License",
  "main": "index.js",
  "bugs": {
    "url": "https://github.com/Hoosat-Oy/hoosat-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Hoosat-Oy/hoosat-template"
  },
  "scripts": {
    "test": "jest",
    "clean": "rm -rf build",
    "create:service-worker:urls": "node build/public/service-worker-urls.js",
    "create:build:directories": "npx mkdirp build && npx mkdirp build/public",
    "move:css": "node --loader ts-node/esm build-tools/extract-css.ts src/client build/public/styles.css",
    "move:css:watch": "chokidar 'src/**/*.css' -c 'npm run move:css'",
    "move:assets": "npx ncp public build/public",
    "move:assets:watch": "chokidar 'public/**/*' -c 'npm run move-assets'",
    "prebuild": "clean create:build:directories move:assets move:css create:service-worker:urls",
    "build:client:dev:watch": "webpack --config webpack.client.config.js --mode=development --watch",
    "build:server:dev:watch": "webpack --config webpack.server.config.js --mode=development --watch",
    "build:client:dev": "webpack --config webpack.client.config.js --mode=development",
    "build:server:dev": "webpack --config webpack.server.config.js --mode=development",
    "build:dev:watch": "npm-run-all -p build:client:dev:watch build:server:dev:watch",
    "build:dev": "npm-run-all -p build:client build:server",
    "run:dev": "nodemon --experimental-modules build/server.js",
    "dev": "npm-run-all prebuild -p move:assets:watch move:css:watch build:dev:watch run:dev",
    "build:client:prod": "webpack --config webpack.client.config.js --mode=production",
    "build:server:prod": "webpack --config webpack.server.config.js --mode=production",
    "build:prod": "npm-run-all build:client:prod build:server:prod",
    "build": "npm-run-all prebuild build:prod",
    "run:prod": "node --experimental-modules build/server.js",
    "start": "npm run run:dev",
    "deploy": "npm run build && rsync --delete --exclude --exclude='.git' --exclude='.env' --exclude='node_modules' -rPz ./ YOURSERVER:~FOLDER"
  },
  "dependencies": {
    "formidable": "^3.5.0",
    "i18next": "^23.4.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-fs-backend": "^2.1.5",
    "i18next-http-backend": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-i18next": "^13.0.3",
    "react-markdown": "^8.0.7",
    "react-router-dom": "6.4",
    "rehype-highlight": "^6.0.0",
    "remark-gfm": "^3.0.1",
    "save-dev": "^0.0.1-security",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/formidable": "^3.4.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^16.11.10",
    "@types/uuid": "^9.0.2",
    "@types/webpack": "^5.28.1",
    "@types/ws": "^8.5.5",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.7.3",
    "esm": "^3.2.25",
    "file-loader": "^6.2.0",
    "jest": "^29.5.0",
    "mkdirp": "^3.0.1",
    "node-loader": "^2.0.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "style-loader": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typedoc": "^0.24.7",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.1",
    "webpack-node-externals": "^3.0.0"
  },
  "keywords": []
}
